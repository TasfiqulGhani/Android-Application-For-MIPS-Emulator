        android:layout_marginLeft="85dp"
        android:layout_marginStart="85dp"
        android:layout_below="@+id/rrone"
        android:layout_toRightOf="@+id/aftershift"
        android:layout_toEndOf="@+id/aftershift" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:id="@+id/befresign"
        android:layout_marginBottom="21dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/rrteo"
        android:layout_toEndOf="@+id/rrteo"
        android:layout_marginLeft="28dp"
        android:layout_marginStart="28dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:id="@+id/aftersign"
        android:layout_alignTop="@+id/befresign"
        android:layout_alignLeft="@+id/rdtwo"
        android:layout_alignStart="@+id/rdtwo" />
</RelativeLayout>
