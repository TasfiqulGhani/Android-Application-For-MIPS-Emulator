        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:id="@+id/rdtwo"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/aluresult"
        android:layout_toLeftOf="@+id/aftershift"
        android:layout_toStartOf="@+id/aftershift" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
