        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:id="@+id/rrteo"
        android:layout_below="@+id/rdone"
        android:layout_alignLeft="@+id/rrone"
        android:layout_alignStart="@+id/rrone" />

    <TextView
